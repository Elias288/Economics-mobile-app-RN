{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "universe/native"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native",
        "jsdoc"
    ],
    "rules": {
        "react-hooks/exhaustive-deps": "warn",
        "jsdoc/require-description": "warn"
    }
}
